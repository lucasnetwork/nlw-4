{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
        
    ],
    "rules": {
        "prettier/prettier":["error",{"endOfLine":"auto"}],
        "import/prefer-default-export":"off",
        "class-methods-use-this":"off",
        "no-undef":"off",
        "import/extensions":"off",
        "import/no-unresolved":"off"
    }
}
